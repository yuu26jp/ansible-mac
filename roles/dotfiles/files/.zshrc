alias g='git'
alias gpl='git pull'
alias gps='git push origin'
alias glog='git log --oneline | head'
alias gh='ghq look $(ghq list | peco)'

alias la='ls -laG'
alias ll='ls -lG'
alias mv='mv -i'
alias cp='cp -i'
alias rm='rm -i'
alias crontab='crontab -i'

export PATH=$PATH:~/.nodebrew/current/bin/
